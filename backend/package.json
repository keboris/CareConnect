{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "imports": {
    "#db": {
      "development": "./src/db/index.ts",
      "default": "./src/db/index.js"
    },
    "#controllers": {
      "development": "./src/controllers/index.ts",
      "default": "./src/controllers/index.js"
    },
    "#docs": {
      "development": "./src/docs/index.ts",
      "default": "./src/docs/index.js"
    },
    "#middlewares": {
      "development": "./src/middlewares/index.ts",
      "default": "./src/middlewares/index.js"
    },
    "#models": {
      "development": "./src/models/index.ts",
      "default": "./src/models/index.js"
    },
    "#routes": {
      "development": "./src/routes/index.ts",
      "default": "./src/routes/index.js"
    },
    "#schemas": {
      "development": "./src/schemas/index.ts",
      "default": "./src/schemas/index.js"
    },
    "#utils": {
      "development": "./src/utils/index.ts",
      "default": "./src/utils/index.js"
    }
  },
  "scripts": {
    "dev": "node --env-file=.env --watch --conditions development src/app.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node --env-file=.env dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "openapi-types": "^12.1.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/typescript": "^0.4.29",
    "typescript": "^5.9.3"
  }
}
